<HEAD>
<TITLE>Cisco Systems Login</TITLE>

<META http-equiv=Content-Type content="text/html; charset=utf-8">

<STYLE type=text/css>
.configLoginScreen .k-button {
	padding: 6px 20px;
	border-radius: 25px;
	color: #232f34;
	border: 1px solid #9ec654;
	outline: 0;
	font-size: 16pt;
	margin-right: 4px;
	min-width: 130px;
	background-color: #9ec654;
	color: #ffffff;
	cursor: pointer;
}

.configLoginScreen {
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	display: block;
	background-color: #E6E9F0;
	text-align: center;
	overflow: auto;
	color: #70717B;
	font-family: CiscoSansThin, "Helvetica Neue", Arial, Tahoma, sans-serif;
}

.configLoginScreen .configLoginScreenContainer {
	margin: auto;
	display: inline-block;
	padding: 1rem;
	width: 500px;
	background-color: #ffffff;
	position: relative;
	top: 50%;
	-webkit-transform: translate(0%, -50%);
	-moz-transform: translate(0%, -50%);
	-ms-transform: translate(0%, -50%);
	transform: translate(0%, -50%);
}

.configLoginScreen .configLoginCompanyLogoWrapper {
	text-align: center;
	white-space: nowrap;
	overflow: hidden;
	font-size: 5.5 rem;
	color: #70717B;
	margin: 70px 0 60px 0;
}

.configLoginScreen .configLoginScreenContainer .configProductName {
	font-size: 38px;
	line-height: 60px;
	padding: 0 5px 20px 5px;
}

.configLoginScreen .configWelcomeMessage {
	font-size: 13px;
}

.configLoginScreen .configLoginScreenContainer .configLoginCopyright {
	margin: 0 0 20px 0;
	font-size: 12px;
	text-align: justify;
}

.configLoginScreen .configLoginScreenContainer .configLoginFields {
	display: inline-block;
	margin: 80px 0;
}



.logintitle {
	FONT-WEIGHT: bold; FONT-SIZE: 12px; COLOR: #ffffff; FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif
}
.login {
	FONT-WEIGHT: normal; FONT-SIZE: 11px; COLOR: #ffffff; FONT-FAMILY: Verdana, Arial, Helvetica, sans-serif
}
.style1 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-weight: bold;
}
TEXTAREA.area{
        COLOR: #4682B4;
        BACKGROUND-COLOR: #FFFFFF;
}
span.copyright {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
        font-weight: bold;
        text-align: left;
	color: #999999;
}

.icon-cisco_logo {
	height: 50px;
	background-image: url('data:image/png;base64,');
	background-repeat: no-repeat;
	background-position: center;
}
</STYLE>
<SCRIPT>
function showLogin() {
	if (document.getElementById('loginDiv').style.display != 'block') {
		document.getElementById('loginDiv').style.display = 'block';
		document.getElementById('userid').value = '';
		document.getElementById('pw').value = '';
		document.getElementById('userid').focus();
	}
}

function cancelLogin() {
	document.getElementById('loginDiv').style.display = 'none';
}

function submitLogin() {
	var username = document.getElementById('userid').value;
	var password = document.getElementById('pw').value;
	if ((username != "") && (password != ""))
	{
	    CHttpFormExtractor.submitIoeLogin(username, password, function (rtn) { cancelLogin(); document.forms["loginForm"].submit(); });
	}
}

function displayLoginFailMessage() {
	document.getElementById('errorDiv').style.display = 'block';
}

function hideLoginFailMessage() {
	document.getElementById('errorDiv').style.display = 'none';
}
</SCRIPT>

<META content="MSHTML 6.00.2900.2180" name=GENERATOR>
</HEAD>
<BODY text=#000000 leftMargin=0 topMargin=0 marginwidth="0" marginheight="0" height="100%">
<div id="loginDiv" style="position: absolute; left: calc(50% - 140px); top: 50px; background-color: lightgray; border-style: solid; border-width: 3px; display:none; z-index: 1000;">
	<form name="loginForm" action="main.html" method="POST">
		<TABLE>
			<TR>
				<TD colspan="2">Authentication Required</TD>
			</TR>
			<TR>
				<TD>User Name:</TD>
				<TD><input value="" maxlength="32" tabindex="0" id="userid" class="inputInner" autocomplete="on" name="userid" type="text"></TD>
			</TR>
			<TR>
				<TD>Password:</TD>
				<TD><input value="" maxlength="80" tabindex="0" id="pw" class="inputInner" autocomplete="on" name="pw" type="password"></TD>
			</TR>
			<TR>
				<TD></TD>
				<TD>
					<INPUT style="cursor:pointer;" type=submit value="Login" name="bSubmit " onclick="submitLogin()">
					<INPUT style="cursor:pointer;" type=button value="Cancel" onclick="cancelLogin()">
				</TD>
			</TR>
		</TABLE>
	</form>
</div>
<div id="errorDiv" style="position: absolute; left: calc(50% - 120px); top: 50px; background-color: lightgray; border-style: solid; border-width: 3px; display:none; z-index: 2000;">
	<TABLE>
		<TR>
			<TD colspan="2">Authentication Error</TD>
		</TR>
		<TR>
		</TR>
		<TR>
			<TD>Invalid User Name or Password.</TD>
		</TR>
		<TR>
			<TD></TD>
			<TD>
				<INPUT style="cursor:pointer;" type=button value="OK" onclick="hideLoginFailMessage()">
			</TD>
		</TR>
	</TABLE>
</div>

<div class="configLoginScreen" valign="middle" id="loginPage">
	<div class="configLoginScreenContainer">
		<div class="configLoginCompanyLogoWrapper">
			<div class="icon-cisco_logo" title="Cisco Logo" id="cisco_logo"></div>
		</div>
		<div class="configProductName ng-binding">
			Wireless LAN Controller
		</div>
		<div class="configWelcomeMessage ng-binding">
			Welcome! Please click the login button to enter your user name and password
		</div>
	   	<p></p>
		<div id="banner" class="configWelcomeMessage">
                </div>
		<div class="configLoginFields">
